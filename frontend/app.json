// For the expo app
{
  "expo": {
    "name": "LearnFitness",
    "slug": "learnfitness",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.LearnFitness.LearnFitness",
      "infoPlist": {
        "CFBundleURLTypes": [
          {
            "CFBundleURLSchemes": [
              "com.googleusercontent.apps.611505217549-e4dc3rks52mml21jat2tm5mp781qtr8j"
            ]
          }
        ]
      }
    },
    "android": {
      "package": "com.LearnFitness.LearnFitness",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "androidStatusBar": {
      "translucent": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": ["@react-native-google-signin/google-signin"],
    "extra": {
      "eas": {
        "projectId": "e8caa024-5e5d-4b7b-876c-8fd1261e6845"
      }
    },
    "owner": "learnfitnessteam"
  }
}
